{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "http://localhost:5100",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:7100;http://localhost:5100",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "watch-http": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "watch --launch-profile http --verbose",
      "workingDirectory": "$(ProjectDir)",
      "launchBrowser": true,
      "launchUrl": "http://localhost:5100/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "watch-https": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "watch --launch-profile https --verbose",
      "workingDirectory": "$(ProjectDir)",
      "launchBrowser": true,
      "launchUrl": "https://localhost:7100/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
